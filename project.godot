; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

[application]

config/name="GIG - Game Idea Generator"
config/description="Need new idea for a game? This app will randomize genre, dimension(3D, 2D, 2.5D) and requirmend!"
run/main_scene="res://Scenes/main.tscn"
config/use_custom_user_dir=true
config/custom_user_dir_name="GIG"
boot_splash/image="res://Icons/controller_icon.png"
config/icon="res://Icons/controller_icon.png"

[display]

window/size/width=320
window/size/height=320
window/size/resizable=false

[physics]

common/enable_pause_aware_picking=true

[rendering]

2d/snapping/use_gpu_pixel_snap=true
environment/default_clear_color=Color( 0.172549, 0.164706, 0.196078, 1 )
environment/default_environment="res://default_env.tres"
